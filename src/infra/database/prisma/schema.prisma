generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Error {
  id                      String   @id @default(uuid())
  exception_was_thrown_in String
  resource_url            String
  http_method             String
  stack                   String
  created_at              DateTime @default(now())

  @@map("errors")
}
